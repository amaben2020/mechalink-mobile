{
  "expo": {
    "name": "Mechalink",
    "slug": "mechalink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/react-logo@3x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/react-logo@3x.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.algomachine.mechalink"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/react-logo@3x.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favreact-logo@3x"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://mechalink.com/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/react-logo@3x.png",
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://mechalink.com/"
      },
      "eas": {
        "projectId": "d94f4331-97c1-4fcc-b63e-ff81b7eba584"
      }
    },
    "owner": "algomachine"
  }
}
