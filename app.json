{
  "expo": {
    "name": "Mechalink",
    "slug": "mechalink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/react-logo@3x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/react-logo@3x.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.algomachine.mechalink",
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/react-logo@3x.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.algomachine.mechalink"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favreact-logo@3x"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://mechalink.com/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/react-logo@3x.png",
          "resizeMode": "contain"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Mechalink to use your location."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/react-logo@3x.png",
          "color": "#ffffff",
          "sounds": [
            "./local/path/to/mySound.wav",
            "./local/path/to/myOtherSound.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://mechalink.com/"
      },
      "eas": {}
    },
    "owner": "algomachine"
  }
}
